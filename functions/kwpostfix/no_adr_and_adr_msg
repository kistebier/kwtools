# Funktion, keine beiden Empfänger eingegeben # {{{
# usage: no_adr_and_adr_msg
no_adr_and_adr_msg() {
	MSG=$(gettext 'Sie müssen erst die Menüpunkte lokaler Empfänger und andere Empfänger belegen.')
	msgbox "$gv_Attention" "$MSG"
}
## }}}
